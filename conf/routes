# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                        controllers.Assets.at(path="/public", file)

POST        /candidate                           controllers.CandidateController.registerCandidate()
GET         /candidate/:email                    controllers.CandidateController.findCandidate(email: String)
GET         /candidate                           controllers.CandidateController.getAll()
GET         /questionnarie/:group                controllers.QuestionnaireController.getQuestionnaire(group: String)
GET         /questionnarie/:email/:groups        controllers.QuestionnaireController.getQuestionnarie(email: String, groups: String)
POST        /questionnaire                       controllers.QuestionnaireController.fillQuestionnarie()